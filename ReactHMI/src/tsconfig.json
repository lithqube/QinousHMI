{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "sourceMap": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "allowJs": true,
        "noImplicitAny": false,
        "strictNullChecks": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "preserveConstEnums": false,
        "removeComments": true,
        "lib": ["es5", "dom", "es2015"]
    }
}